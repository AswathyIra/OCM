<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ocm">

    <application
        android:name=".OCMApp"
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.RegisterActivity" />
        <activity android:name=".Activity.HomeActivity" />
        <activity android:name=".Activity.AdminHomeActivity" />
        <activity android:name=".Activity.AddAndViewActivity" />
        <activity android:name=".Activity.AddSubDataActivity" />
        <activity android:name=".Activity.ViewAndApproveUserActivity"></activity>
    </application>

</manifest>